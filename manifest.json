{
    "name": "FocusMan",
    "version": "1.0",
    "description": "a simple extension for be more concentrate",
    "manifest_version": 3,
  
    "permissions": [
      "activeTab",
      "storage",
      "scripting"
    ],
  
    "action": {
      "default_popup": "popup.html"
    },
  
    "icons": {
      "16": "focusman-logo.png",
      "32": "focusman-logo.png",
      "48": "focusman-logo.png",
      "128": "focusman-logo.png"
    }
  }
  